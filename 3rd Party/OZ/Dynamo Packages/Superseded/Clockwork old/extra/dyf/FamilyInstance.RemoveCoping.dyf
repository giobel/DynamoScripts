<Workspace Version="0.7.4.3192" X="213.050452475923" Y="210.363774866475" zoom="1.02988563947099" Description="Removes a coping (cut) from a steel beam" Category="Clockwork.Revit.Elements.FamilyInstance.Actions" Name="FamilyInstance.RemoveCoping" ID="7ebb8ad1-2aed-4147-810c-4cd0b15f84b2">
  <Elements>
    <DSIronPythonNode.PythonNode type="DSIronPythonNode.PythonNode" guid="bd5723c9-8cff-47d0-b53e-e5f6a5e2a60b" nickname="Python Script" x="450.919744724847" y="44.7780951658517" isVisible="true" isUpstreamVisible="true" lacing="Disabled" inputcount="2">
      <Script>import clr
import System
clr.AddReference('RevitAPI')
from Autodesk.Revit.DB import *

clr.AddReference("RevitServices")
import RevitServices
from RevitServices.Persistence import DocumentManager
from RevitServices.Transactions import TransactionManager

doc = DocumentManager.Instance.CurrentDBDocument
fams = UnwrapElement(IN[0])
cutters = UnwrapElement(IN[1])
booleans = list()
counter = 0

TransactionManager.Instance.EnsureInTransaction(doc)
for fam in fams:
	try:
		fam.RemoveCoping(cutters[counter])
		booleans.append(True)
	except:	booleans.append(False)
	counter += 1
TransactionManager.Instance.TransactionTaskDone()

OUT = booleans</Script>
